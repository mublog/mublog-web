{
  "compilerOptions": {
    "module": "esnext",
    "jsxFactory": "Doc.h",
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "allowJs": true,
    "allowUnreachableCode": true,
    "alwaysStrict": true,
    "checkJs": true
  }
}